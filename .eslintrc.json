{
    "env": {
        "commonjs": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-console": ["error", { "allow": ["info", "warn", "error"] }],
        "strict": "off",
        "no-await-in-loop": "off",
        "no-restricted-syntax": "off",
        "max-len": "off"
    }
}